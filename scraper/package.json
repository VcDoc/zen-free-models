{
  "name": "zen-free-models-scraper",
  "version": "1.0.0",
  "description": "Scrape OpenCode Zen docs for free models",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "dev": "tsx src/index.ts",
    "test": "node --import tsx --test tests/output.test.ts",
    "test:unit": "node --import tsx --test tests/matching.test.ts",
    "test:all": "node --import tsx --test tests/*.test.ts",
    "test:integration": "tsx tests/matching.integration.test.ts",
    "coverage": "node --import tsx --test --experimental-test-coverage tests/*.test.ts",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "pretest": "pnpm run lint && pnpm run format:check"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^3.0.0",
    "dotenv": "^17.2.3",
    "openai": "^6.16.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.8.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.53.0"
  },
  "packageManager": "pnpm@10.28.0"
}
