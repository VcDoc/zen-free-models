# Required: Browserbase credentials for browser automation
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# Required: OpenAI API key for Stagehand LLM extraction and model matching
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Stagehand verbosity level (0=silent, 1=normal, 2=verbose)
# Default: 1
STAGEHAND_VERBOSE=1

# Optional: Log level (silent, error, warn, info, debug)
# Default: info
LOG_LEVEL=info

# Optional: URLs (override for testing)
# ZEN_API_URL=https://opencode.ai/zen/v1/models
# ZEN_DOCS_URL=https://opencode.ai/docs/zen/

# Optional: LLM models (override defaults)
# STAGEHAND_MODEL=openai/gpt-5-mini
# MATCHING_MODEL=gpt-5-mini

# Optional: LLM service tier (default, flex, auto, scale, priority)
# Default: flex (for cost savings)
# LLM_SERVICE_TIER=flex

# Optional: Retry configuration
# MAX_RETRIES=3
# INITIAL_DELAY_MS=1000

# Optional: Timeout configuration (in milliseconds)
# STAGEHAND_INIT_TIMEOUT_MS=60000
# STAGEHAND_CLOSE_TIMEOUT_MS=10000
